{
  "env": {
    "es2021": true
  },
  "plugins": ["jest"],
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "jest/expect-expect": ["error", { "assertFunctionNames": ["expect", "request.**.expect"] }]
  }
}
